{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "cadinho",
  "version": "0.1.0",
  "identifier": "net.mangatrix.cadinho",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Cadinho",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "maximized": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/cadinho-32x32.png",
      "icons/cadinho-128x128.png",
      "icons/cadinho.png",
      "icons/cadinho.ico"
    ]
  }
}
